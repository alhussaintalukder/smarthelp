# ─────────────────────────────────────────────────────────────────────────────
# SmartHelp — example environment variables
#
# Copy this file to .env and fill in your real values.
# .env is gitignored; this file is safe to commit.
# ─────────────────────────────────────────────────────────────────────────────

# ── Firebase ──────────────────────────────────────────────────────────────────
# Firebase Console → Project Settings → Your apps → Web app
EXPO_PUBLIC_FIREBASE_API_KEY=
EXPO_PUBLIC_FIREBASE_AUTH_DOMAIN=
EXPO_PUBLIC_FIREBASE_DATABASE_URL=
EXPO_PUBLIC_FIREBASE_PROJECT_ID=
EXPO_PUBLIC_FIREBASE_STORAGE_BUCKET=
EXPO_PUBLIC_FIREBASE_MESSAGING_SENDER_ID=
EXPO_PUBLIC_FIREBASE_APP_ID=

# ── Agora ─────────────────────────────────────────────────────────────────────
# https://console.agora.io → Project Management
EXPO_PUBLIC_AGORA_APP_ID=
EXPO_PUBLIC_AGORA_APP_CERTIFICATE=

# Token server URL (leave blank for App-ID-only dev mode)
EXPO_PUBLIC_AGORA_TOKEN_SERVER_URL=
